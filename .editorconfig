# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{cs|csx}]
indent_style = tab
max_line_length = 88
# enforce naming rules
dotnet_diagnostic.IDE1006.severity = warning
